{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "typeRoots": [
      "./env.d.ts"
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "core/components/*"
      ],
      "@hooks/*": [
        "core/hooks/*"
      ],
      "@interfaces/*": [
        "core/interfaces/*"
      ],
      "@utils/*": [
        "core/utils/*"
      ]
    }
  }
}