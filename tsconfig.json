{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "types": [
      "react-native-dotenv"
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "core/components/*"
      ],
      "@hooks/*": [
        "core/hooks/*"
      ],
      "@interfaces/*": [
        "core/interfaces/*"
      ],
      "@utils/*": [
        "core/utils/*"
      ],
      "@views/*": [
        "core/views/*"
      ]
    }
  }
}